<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- OAuth 2.0 Configuration (Required) -->
		<!-- Get OAuth credentials from: https://console.cloud.google.com/apis/credentials -->
		<!-- When creating OAuth 2.0 Client ID, select "Desktop app" as the Application type -->
		<!-- Google will automatically configure http://localhost as an authorized redirect URI -->
		<add key="GOOGLE_CLIENT_ID" value=""/>
		<add key="GOOGLE_CLIENT_SECRET" value=""/>

		<!-- DEPRECATED: Redirect URI is not needed for Desktop apps -->
		<!-- The Google.Apis.Auth library automatically handles localhost redirects -->
		<!-- Leave this empty or remove it -->
		<!-- <add key="REDIRECT_URI" value=""/> -->

		<!-- OPTION 2: API Key (Fallback, less secure - currently not supported) -->
		<!-- Get your API key from: https://aistudio.google.com/app/apikey -->
		<!-- <add key="GEMINI_API_KEY" value="YOUR_API_KEY_HERE"/> -->
	</appSettings>
</configuration>
