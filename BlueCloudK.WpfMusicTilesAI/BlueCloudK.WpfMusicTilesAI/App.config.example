<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Gemini API Key Configuration (Required for beat map generation) -->
		<!-- Get your FREE API key from: https://aistudio.google.com/app/apikey -->
		<!--
		INSTRUCTIONS:
		1. Visit https://aistudio.google.com/app/apikey
		2. Sign in with your Google account
		3. Click "Create API key"
		4. Copy the API key
		5. Paste it below (replace the empty value="")
		6. Save this file
		7. Restart the application

		Each user should create their own API key using their Google account.
		This allows each user to use their own Gemini API quota (free tier available).
		-->
		<add key="GEMINI_API_KEY" value=""/>

		<!-- Gemini Model Selection (Optional) -->
		<!-- Choose which Gemini model to use for beat map generation -->
		<!-- Options:
		     - gemini-2.0-flash-exp (Default, fastest, experimental)
		     - gemini-1.5-flash (Fast, stable)
		     - gemini-1.5-pro (Best quality, slower)
		-->
		<add key="GEMINI_MODEL" value="gemini-2.0-flash-exp"/>

		<!-- YouTube Data API Key (Optional, for YouTube Music search) -->
		<!-- Get from: https://console.cloud.google.com/apis/credentials -->
		<add key="YOUTUBE_API_KEY" value=""/>
	</appSettings>
</configuration>
