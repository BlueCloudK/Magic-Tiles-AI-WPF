<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- OAuth 2.0 Configuration (Required) -->
		<!-- Get OAuth credentials from: https://console.cloud.google.com/apis/credentials -->
		<add key="GOOGLE_CLIENT_ID" value=""/>
		<add key="GOOGLE_CLIENT_SECRET" value=""/>

		<!-- Redirect URI for OAuth 2.0 callback -->
		<!-- This must match the redirect URI configured in Google Cloud Console -->
		<!-- For desktop apps, use: http://127.0.0.1:[PORT]/signin-google -->
		<add key="REDIRECT_URI" value="http://127.0.0.1:58291/signin-google"/>

		<!-- OPTION 2: API Key (Fallback, less secure - currently not supported) -->
		<!-- Get your API key from: https://aistudio.google.com/app/apikey -->
		<!-- <add key="GEMINI_API_KEY" value="YOUR_API_KEY_HERE"/> -->
	</appSettings>
</configuration>
